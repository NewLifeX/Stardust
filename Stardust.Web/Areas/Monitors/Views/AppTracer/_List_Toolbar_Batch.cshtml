@using NewLife.Common;
@{
    var user = ViewBag.User as IUser ?? User.Identity as IUser;
    var fact = ViewBag.Factory as IEntityFactory;
    var set = ViewBag.PageSetting as PageSetting;
}
@if (set.EnableSelect)
{
    <button type="button" class="btn btn-purple btn-sm" data-action="action" data-url="@Url.Action("Fix")" data-fields="keys" disabled>
        批量修正
    </button>
    <button type="button" class="btn btn-purple btn-sm" data-action="action" data-url="@Url.Action("FixOldData")" data-fields="keys" disabled>
        修正旧数据
    </button>
}