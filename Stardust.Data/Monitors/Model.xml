    <NameFormat>Default</NameFormat>
    <!--魔方区域显示名-->
    <DisplayName>监控中心</DisplayName>
    <!--魔方控制器输出目录-->
    <CubeOutput>../../Stardust.Web/Areas/Monitors/</CubeOutput>
